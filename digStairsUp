for i=1,30 do
  while(turtle.detect()) do 
   turtle.dig()
  end
  while(turtle.detectUp()) do
    turtle.digUp()
  end
  turtle.digDown()
  turtle.down()
  turtle.digDown()
  turtle.placeDown()
  turtle.up()
  turtle.forward()
  while(turtle.detectUp()) do
    turtle.digUp()
  end
  turtle.up()
end

  
